<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
    http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="pizza" class="platform.codingnomads.co.corespring.lab.FoodProduct">
        <property name="brand" value="Gino's"/>
        <property name="food" value="Pepperoni Pizza"/>
    </bean>

    <bean id="quesadilla" class="platform.codingnomads.co.corespring.lab.FoodProduct">
        <property name="brand" value="Goya"/>
        <property name="food" value="Quesadilla"/>
    </bean>
</beans>